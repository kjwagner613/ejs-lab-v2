<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Menu</title>
</head>
<body>
    <%- include('./partials/nav') %>
    <h1>Full Menu</h1>
    <ul>
      <% restaurant.menu.forEach(item => { %>
        <li>
          <strong><%= item.name %></strong> - $<%= item.price %> - Rating: <%= item.rating %>/5 
          <% if (item.rating === 5) { %>
            ‚≠ê
          <% } %>
          <p><%= item.details %></p>
        </li>
      <% }) %>
    </ul>
</body>
</html>

